<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
   
     <servlet>
        <servlet-name>reg_from</servlet-name>
        <servlet-class>reg_form</servlet-class> <!-- Replace com.example.RegistrationFormServlet with your actual servlet class name -->
    </servlet>
    <servlet-mapping>
        <servlet-name>reg_from</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class> <!-- Replace com.example.LoginServlet with your actual servlet class name -->
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>reg_check</servlet-name>
        <servlet-class>reg_check</servlet-class> <!-- Replace com.example.LoginServlet with your actual servlet class name -->
    </servlet>
    <servlet-mapping>
        <servlet-name>reg_check</servlet-name>
        <url-pattern>/reg_check</url-pattern>
    </servlet-mapping>
    
  
     
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file> <!-- Remove unnecessary whitespaces around index.jsp -->
    </welcome-file-list>
    <session-config>
        <session-timeout>30</session-timeout> <!-- Remove unnecessary whitespaces around 30 -->
    </session-config>
</web-app>
